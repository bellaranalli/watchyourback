<div>
  <h2>LISTADO DE PRODUCTOS</h2>
</div>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Title</th>
      <th>Description</th>
      <th>Price</th>
      <th>Category</th>
      <th>Code</th>
    </tr>
  </thead>
  <tbody>
    {{#each product}}
      <tr>
        <td>{{id}}</td>
        <td>{{title}}</td>
        <td>{{description}}</td>
        <td>{{price}}</td>
        <td>{{category}}</td>
        <td>{{code}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>