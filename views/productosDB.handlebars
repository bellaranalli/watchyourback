<div>
  <h2>PRODUCTOS ACTUALES</h2>
</div>

<table>
  <thead>
    <tr>
      <th>NAME</th>
      <th>PRICE</th>
      <th>CATEGORY</th>
      <th>DESCRIPTION</th>
      <th>STOCK</th>
    </tr>
  </thead>
  <tbody>
    {{#each products}}
      <tr>
        <td>{{this.name}}</td>
        <td>{{this.price}}</td>
        <td>{{this.category}}</td>
        <td>{{this.description}}</td>
        <td>{{this.stock}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>

